<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Connect">
   <children>
      <Label fx:id="title" prefHeight="17.0" prefWidth="600.0" text="connect to remote game">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Label fx:id="enter_port" layoutX="5.0" layoutY="110.0" prefHeight="17.0" prefWidth="600.0" text="Enter port">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Label fx:id="enter_ip" layoutX="5.0" layoutY="55.0" prefHeight="17.0" prefWidth="600.0" text="Enter ip">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Label fx:id="enter_name" layoutX="8.0" layoutY="173.0" prefHeight="17.0" prefWidth="600.0" text="Enter name">
         <font>
            <Font size="38.0" />
         </font>
      </Label>
      <Button fx:id="connect" layoutX="482.0" layoutY="317.0" mnemonicParsing="false" onAction="#initialize" prefHeight="55.0" prefWidth="103.0" text="connect" />
      <TextField fx:id="ip" layoutX="230.0" layoutY="70.0" />
      <TextField fx:id="name" layoutX="234.0" layoutY="188.0" />
      <TextField fx:id="port" layoutX="234.0" layoutY="125.0" />
   </children>
</AnchorPane>
