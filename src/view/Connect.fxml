<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ConnectController">
   <children>
      <TextField fx:id="ip" layoutX="230.0" layoutY="103.0" prefHeight="36.0" prefWidth="173.0" />
      <TextField fx:id="name" layoutX="230.0" layoutY="250.0" prefHeight="36.0" prefWidth="173.0" />
      <TextField fx:id="port" layoutX="230.0" layoutY="175.0" prefHeight="36.0" prefWidth="173.0" />
      <Label fx:id="title" layoutX="114.0" layoutY="14.0" prefHeight="55.0" prefWidth="422.0" text="connect to remote game" textFill="#331530">
         <font>
            <Font name="Bernard MT Condensed" size="40.0" />
         </font>
      </Label>
      <ImageView fx:id="enter_ip" fitHeight="55.0" fitWidth="90.0" layoutX="69.0" layoutY="84.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="enter_port" fitHeight="55.0" fitWidth="173.0" layoutX="27.0" layoutY="156.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="enter_name" fitHeight="66.0" fitWidth="187.0" layoutX="20.0" layoutY="235.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="connect" layoutX="461.0" layoutY="312.0" mnemonicParsing="false" onAction="#initialize" prefHeight="55.0" prefWidth="103.0" text="connect" textFill="#141314">
         <font>
            <Font name="System Italic" size="20.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
