<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ConnectController">
   <children>
      <Label fx:id="title" layoutX="106.0" layoutY="14.0" prefHeight="55.0" prefWidth="422.0" text="connect to remote game" textFill="#ba17a9">
         <font>
            <Font name="Bell MT" size="40.0" />
         </font>
      </Label>
      <Label fx:id="enter_port" layoutX="6.0" layoutY="146.0" prefHeight="55.0" prefWidth="214.0" text="Enter port">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Label fx:id="enter_ip" layoutX="9.0" layoutY="74.0" prefHeight="55.0" prefWidth="137.0" text="Enter ip">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Label fx:id="enter_name" layoutX="9.0" layoutY="219.0" prefHeight="55.0" prefWidth="207.0" text="Enter name">
         <font>
            <Font size="33.0" />
         </font>
      </Label>
      <Button fx:id="connect" layoutX="460.0" layoutY="317.0" mnemonicParsing="false" onAction="#initialize" prefHeight="55.0" prefWidth="103.0" text="connect" textFill="#c50eb6">
         <font>
            <Font name="System Italic" size="20.0" />
         </font></Button>
      <TextField fx:id="ip" layoutX="230.0" layoutY="83.0" prefHeight="36.0" prefWidth="173.0" />
      <TextField fx:id="name" layoutX="230.0" layoutY="228.0" prefHeight="36.0" prefWidth="173.0" />
      <TextField fx:id="port" layoutX="230.0" layoutY="155.0" prefHeight="36.0" prefWidth="173.0" />
   </children>
</AnchorPane>
